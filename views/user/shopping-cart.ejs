</head>

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header-section">
        <div class="container">
            <div class="inner-header">
                <div class="row">
                    <div class="col-lg-1 col-md-1">
                        <div class="logo">
                            <a href="/">
                                <img src="/img/logo.png" alt="">
                            </a>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6">
                        <form method="GET" action='/search'>

                            <div class="advanced-search d-flex flex-row">
                                <div class="input-group">
                                    <form>
                                    <input type="text" name="searchquery" placeholder="What do you need?">
                                </div>
                                <div class="input-group">
                                    <button type="submit"><i class="ti-search"></i></button>
                                </div>
                            </div>
                        </form>
                    </div>

             

                    <div class="col-lg-5 text-right col-md-5">
                        <ul class="nav-right">
                            
                            <li class="cart-icon">
                                <a href="/shopping-cart">Shopping Cart
                                    <i class="fa fa-shopping-cart"></i>
                                   <% if(!((session==undefined)|| (session.cart==undefined))){ %>
                                    <span class="badge"><%=session.cart.totalQty%> </span>
                                   <% }%>
                                    <!--may be ignored for a while
                                    <span>3</span>
                                -->
                                </a>
                            </li>
                          <%  if(user){%>
                                <li><a href="/user/logout" class="login-panel"><i class="fa fa-user mr-1"></i>Logout</a></li>
                                <li><%=user.name%></li>
                                <%}else{%>
                                <li><a href="/user/signin" class="login-panel"><i class="fa fa-user mr-1"></i>Login</a></li>
                                <%} %>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="nav-item">
            <div class="container">
                <nav class="nav-menu mobile-menu">
                    <ul>
                        <li class="active"><a href="/">Home</a></li>
                        <li  ><a href="/all-products">All Products</a></li>
                        <li><a href="/#brandsection">Brands</a></li>
                        <li><a href="/#prodsection">Supplements</a></li>
                        <li><a href="/contact">Contact Us</a>
                            <!--
                            <ul class="dropdown">
                                <li><a href="#">Men</a></li>
                            </ul>
                        -->
                        </li>
                    </ul>
                </nav>
                <div id="mobile-menu-wrap"></div>
            </div>
        </div>
    </header>
    <!-- Header End -->

    <!-- Breadcrumb Section Begin -->
    <div class="breacrumb-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb-text product-more">
                        <a href="/"><i class="fa fa-home"></i> Home</a>
                        <a href="/all-products">Shop</a>
                        <span>Shopping Cart</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Breadcrumb Section Begin -->

    <!-- Shopping Cart Section Begin -->
    <section class="shopping-cart spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <% if(products==null){ %>
                        <h2 class="text-center"> Cart Is Empty </h2>
                        <% }else{ %>
                    <div class="cart-table">
                        <table>
                            <thead>
                                <tr>
                                    <th>Image</th>
                                    <th class="p-name">Product Name</th>
                                    <th>Price</th>
                                    <th>Quantity</th>
                                    <th>Total</th>
                                    <th><i class="ti-close"></i></th>
                                </tr>
                            </thead>
                            <tbody>
                                <%products.forEach(function(product){ %>
                                    
                            
                                <tr>
                                    <td class="cart-pic  first-row"><img class="" src="/uploads/<%=product.item.image_path%>" alt=""></td>
                                    <td class="cart-title first-row">
                                        <h5><%=product.item.product_name%></h5>
                                    </td>
                                    <td class="p-price first-row"><%=product.item.discounted_price%></td>
                                    <td class="p-price first-row">
                                        <!-- <div class="quantity"> -->
                                            <!-- <div class="pro-qty"> -->
                                            <!-- </div> -->
                                        <!-- </div> -->
                                        <a href="/reduce/<%=product.item._id%>"> <i class="fa fa-minus"></i> </a>
                                        <%=product.qty%>
                                     
                                        <a href="/add-to-cart/rqst-from-cart-page/<%=product.item._id%>/"> <i class="fa fa-plus"></i> </a>

                                    </td>
                                    
                                    <td class="total-price first-row"><%=product.price%></td>
                                    
                                    <td class="close-td first-row">
                                        <a href="/remove/<%=product.item._id%>/"> 
                                    
                                        <i class="ti-close"></i>
                                    </a>
                                    </td>
                                </tr>
                              
                                <%   }); %>

                            </tbody>
                        </table>
                    </div>


             <%   } %>
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="cart-buttons">
                                <a href="/all-products" class="primary-btn continue-shop">Continue shopping</a>
                    <% if(!(products==null)){ %>

                                <!-- <a href="#" class="primary-btn up-cart">Update cart</a> -->
             <%   } %>

                            </div>
                    <% if(!(products==null)){ %>

                            <!-- <div class="discount-coupon">
                                <h6>Discount Codes</h6>
                                <form action="#" class="coupon-form">
                                    <input type="text" placeholder="Enter your codes">
                                    <button type="submit" class="site-btn coupon-btn">Apply</button>
                                </form>
                            </div> -->
             <%   } %>

                        </div>
                    <% if(!(products==null)){ %>
                        
                        <div class="col-lg-4 offset-lg-4">
                            <div class="proceed-checkout">
                                <ul>
                                    <li class="subtotal">Subtotal <span><%= totalPrice %></span></li>
                                    <li class="cart-total">Total <span id="total-price" ><%= totalPrice %></span></li>
                                </ul>
                                <a href="/checkout" class="proceed-btn">PROCEED TO CHECK OUT</a>
                            </div>
                        </div>
             <%   } %>
             
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Shopping Cart Section End -->
   
    <!-- Partner Logo Section Begin -->
    <div class="partner-logo">
        <div class="container">
            <div class="logo-carousel owl-carousel">
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="img/logo-carousel/logo-1.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="img/logo-carousel/logo-2.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="img/logo-carousel/logo-3.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="img/logo-carousel/logo-4.png" alt="">
                    </div>
                </div>
                <div class="logo-item">
                    <div class="tablecell-inner">
                        <img src="img/logo-carousel/logo-5.png" alt="">
                    </div>
                </div>
            </div>
        </div>
    </div>
