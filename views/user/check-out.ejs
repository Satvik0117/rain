    <!-- Breadcrumb Section Begin -->

    <!-- Shopping Cart Section Begin -->
    <section class="checkout-section spad">
        <div class="container">
            <div class="checkout-form">
                <div class="row">
                    <div class="col-lg-6">
                        <!-- <div class="checkout-content">
                            <a href="#" class="content-btn">Click Here To Login</a>
                        </div> -->
                        <h4>Biiling Details</h4>
                        <div class="row">
                        <p> To change any of the above details, click here. </p>
                        <a href="/user/profile/edit">Edit<i class="fa fa-edit"></i></a>

                        </div>
                        <div class="row">
                            <div class="col-lg-6">
                                <label for="fir">Name<span>*</span></label>
                                <input  style="background-color: #f3f3f3;"  value="<%=user.name%>" readonly type="text" id="fir">
                            </div>
                            
                            <div class="col-lg-12">
                                <label for="street">Address<span>*</span></label>
                                <input type="text" style="background-color: #f3f3f3;" value="<%=user.address%>" readonly  id="street" class="street-first">
                            </div>
                            
                            <div class="col-lg-6">
                                <label for="email">Email Address<span>*</span></label>
                                <input type="text" style="background-color: #f3f3f3;" value="<%=user.email%>" readonly  id="email">
                            </div>
                            <div class="col-lg-6">
                                <label for="phone">Phone<span>*</span></label>
                                <input type="text" style="background-color: #f3f3f3;" value="<%=user.number%>" readonly  id="phone">
                            </div>
                            <div class="col-lg-12">

                               
                            </div>
                                <!-- <div class="col-lg-12">
                                    <div class="create-item">
                                        <label for="acc-create">
                                            Create an account?
                                            <input type="checkbox" id="acc-create">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div> -->
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <!-- <div class="checkout-content">
                            <input type="text" placeholder="Enter Your Coupon Code">
                        </div> -->
                        <div class="place-order">
                            <h4>Your Order</h4>
                            <div class="order-total">
                                <ul class="order-table">
                                    <li>Product <span>Total</span></li>
                                    <%products.forEach(function(product){ %>

                                    <li class="fw-normal"><%=product.item.product_name%> x <%=product.qty%> <span><%=product.price%></span></li>
                                    
                                <%   }); %>

                                    <!-- <li class="fw-normal">Combination x 1 <span>$60.00</span></li>
                                    <li class="fw-normal">Combination x 1 <span>$120.00</span></li> -->
                                    <li class="fw-normal">Subtotal <span><%=totalPrice%></span></li>
                                    <li class="total-price">Total <span id="total_price"><%=totalPrice%></span></li>
                                </ul>
                                <!-- <div class="payment-check">
                                    <div class="pc-item">
                                        <label for="pc-check">
                                            Cheque Payment
                                            <input type="checkbox" id="pc-check">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                    <div class="pc-item">
                                        <label for="pc-paypal">
                                            Paypal
                                            <input type="checkbox" id="pc-paypal">
                                            <span class="checkmark"></span>
                                        </label>
                                    </div>
                                </div> -->

                                <form action="/payment/success/" method="POST">
                                    <script
                                        src="https://checkout.razorpay.com/v1/checkout.js"
                                        data-key="rzp_test_Ih0kcWAAvF8Jcx" // Enter the Key ID generated from the Dashboard
                                        data-amount=<%=totalPrice*100%> // Amount is in currency subunits. Default currency is INR. Hence, 50000 means 50000 paise or INR 500.
                                        data-currency="INR"
                                        data-buttontext="Proceed to Pay"
                                        data-name="PSN"
                                        data-description="A premier nutrition company"
                                        data-image="/img/logo.png"
                                        data-prefill.name=<%=user.name%>
                                                                data-prefill.email=<%=user.email%>
                                                                data-prefill.contact=<%=user.number%>
                                        data-theme.color="#F37254"
                                    ></script>
                                    <input type="hidden" custom="Hidden Element" name="hidden">
                                    </form>
                                

                            </div>
                                <!-- <div class="order-btn">
                                    <button type="submit" disabled='false' class="site-btn place-btn">Place Order</button>
                                </div> -->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            
        </div>
        
    </section>
    
 
    
